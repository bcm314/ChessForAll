<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/lvitem_background"
  	android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="1dip" >

<TextView android:id="@+id/text1"
	style="@style/c4aTV1"
	android:layout_alignParentLeft="true"
	android:paddingLeft="3dip"
	android:layout_marginTop="0dip"
	android:layout_marginLeft="0dip"
	android:singleLine="true"
	android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text1p"
	style="@style/c4aTV1"
	android:layout_toRightOf="@+id/text1"
	android:paddingLeft="3dip"
	android:layout_marginTop="0dip"
	android:layout_marginLeft="0dip"
	android:singleLine="true"
	android:text="."
	android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text2"
	style="@style/c4aTV1"
	android:layout_toRightOf="@+id/text1p"
	android:paddingLeft="3dip"
	android:layout_marginTop="0dip"
	android:layout_marginLeft="2dip"
	android:singleLine="true"
	android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text2b"
	style="@style/c4aTV1"
	android:layout_toRightOf="@+id/text2"
	android:paddingLeft="3dip"
	android:layout_marginTop="0dip"
	android:layout_marginLeft="0dip"
	android:singleLine="true"
	android:text=" - "
	android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text3"
    style="@style/c4aTV1"
	android:layout_toRightOf="@+id/text2b"
    android:paddingRight="3dip"
    android:layout_marginTop="0dip"
    android:layout_marginLeft="0dip"
    android:singleLine="true"
    android:textColor="@color/text_yellow" />

<TextView android:id="@+id/text6"
	style="@style/c4aTV1"
	android:layout_below="@id/text1"
	android:layout_alignParentRight="true"
	android:paddingRight="3dip"
	android:layout_marginTop="0dip"
	android:layout_marginLeft="10dip"
	android:singleLine="true"
	android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text5"
    style="@style/c4aTV1"
    android:layout_below="@id/text1"
	android:layout_toLeftOf="@+id/text6"
    android:paddingRight="3dip"
    android:layout_marginTop="0dip"
    android:layout_marginLeft="0dip"
    android:singleLine="true"
    android:textColor="@color/text_yellow" />
<TextView android:id="@+id/text4"
    style="@style/c4aTV1"
    android:layout_below="@id/text1"
    android:layout_toLeftOf="@+id/text5"
    android:layout_alignParentLeft="true"
    android:paddingLeft="3dip"
    android:layout_marginTop="0dip"
    android:layout_marginLeft="0dip"
    android:singleLine="true"
    android:textColor="@color/text_yellow" />
</RelativeLayout>
